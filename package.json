{
  "name": "speere",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./grammar": {
      "types": "./dist/grammar.d.ts",
      "import": "./dist/grammar.mjs",
      "require": "./dist/grammar.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
